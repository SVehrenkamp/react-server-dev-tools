{
  "name": "server-devtools-monorepo",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter @server-devtools/ui dev",
    "lint": "pnpm -r lint",
    "smoke": "pnpm --filter @server-devtools/instrumentation build && node scripts/smoke-log-flow.mjs",
    "test:custom-wrapper": "pnpm --filter @server-devtools/instrumentation build && node scripts/test-custom-wrapper.mjs",
    "test": "pnpm -r test",
    "typecheck": "pnpm -r typecheck"
  }
}
